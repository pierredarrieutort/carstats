{
  "name": "carstats",
  "version": "1.0.0",
  "author": "pierredarrieutort, mathieudaix",
  "description": "the new driving experience.",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "server": "nodemon --exec babel-node server.js",
    "serverprod": "babel-node server.js",
    "client": "parcel watch public/images/* public/scripts/app/barbaRouter.js public/scripts/**/index.js public/styles/index.scss --hmr-port 8080",
    "preclientprod": "rimraf dist .parcel-cache && echo 'Cleaned Production folders'",
    "clientprod": "parcel build public/images/* public/scripts/app/barbaRouter.js public/scripts/**/index.js public/styles/index.scss --no-source-maps",
    "prestart": "npx parcel build public/images/* public/scripts/app/barbaRouter.js public/scripts/**/index.js public/styles/index.scss --no-source-maps",
    "start": "babel-node server.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@parcel/config-default": "*",
    "@parcel/transformer-raw": "*",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.6",
    "express": "5.0.0-alpha.8",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "parcel": "*",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.1",
    "workbox-build": "^6.1.5"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",

{
  "name": "carstats",
  "version": "1.0.0",
  "author": "pierredarrieutort, mathieudaix",
  "description": "the new driving experience.",
  "engines": {
    "node": "15.x",
    "npm": "6.x.x"
  },
  "scripts": {
    "preserver": "node -r esm swBuild.js",
    "server": "nodemon -r esm server.js",

    "client": "parcel watch public/images/* public/scripts/**/index.js public/styles/index.scss --no-source-maps --hmr-port 8080",

    "prebuild": "npx rimraf dist",
    "build": "npx parcel build public/images/* public/scripts/**/index.js public/styles/index.scss --no-source-maps",

    "prestart": "node -r esm swBuild.js",
    "start": "node -r esm server.js"
  },
  "license": "ISC",

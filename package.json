    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.6",
    "express": "5.0.0-alpha.8",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "parcel": "*",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.1",
    "workbox-build": "^6.1.5"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-directions": "^4.1.0",
